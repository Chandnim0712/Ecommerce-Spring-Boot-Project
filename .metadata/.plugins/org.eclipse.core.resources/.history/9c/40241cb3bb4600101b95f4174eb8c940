package com.ecom.model;

import jakarta.persistence.Column;

@Entity
public class product {
	
  @Id
private int id;
  
  @Column(length = 500)
  private String title;
  
  @Column(length = 500)
  private String description;
  
  private String category;
  private Double price;
  private int stock;
  private String image;
}
